{
  "name": "generator-humpback",
  "version": "2.0.1",
  "description": "Generate a Drupal project using humpback",
  "homepage": "https://humpbackdev.com",
  "author": {
    "name": "Kevin Porras",
    "email": "kporras07@gmail.com",
    "url": "https://www.twitter.com/kporras07"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "docker",
    "drupal",
    "humpback",
    "containers",
    "cms",
    "manati",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^27.0.6",
    "jest-cli": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^2.6.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "https": "^1.0.0",
    "lodash": "^4.17.15",
    "tunnel-agent": "^0.6.0",
    "uuid": "^8.0.0",
    "xml2js": "^0.4.23",
    "yeoman-generator": "^4.10.1",
    "yeoman-remote": "https://github.com/dharizza/yeoman-remote.git",
    "yosay": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "humpbackdev/generator-humpback",
  "license": "GPL-3.0"
}

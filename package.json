{
  "name": "generator-humpback",
  "version": "0.0.12",
  "description": "Generate a Drupal project using humpback",
  "homepage": "https://humpbackdev.com",
  "author": {
    "name": "Kevin Porras",
    "email": "kporras07@gmail.com",
    "url": "https://www.twitter.com/kporras07"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "docker",
    "drupal",
    "humpback",
    "containers",
    "cms",
    "manati",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.6.0",
    "yeoman-assert": "^3.0.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "https": "^1.0.0",
    "lodash": "^4.17.4",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.19",
    "yeoman-generator": "^2.0.1",
    "yeoman-remote": "^1.0.1",
    "yosay": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "humpbackdev/generator-humpback",
  "license": "GPL-3.0"
}
